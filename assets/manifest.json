{
  "name": "Discogs add to collection",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Add cds to HoG and Ingvill collection",
  "icons": { "128": "disocgs.png" },
  "web_accessible_resources": ["addToCollection.js", "filterSellers.js", "addWantList.js", "filterRelease.js"],
  "content_scripts": [
    {
      "matches": [
        "https://www.discogs.com/*",
        "http://www.discogs.com/*",
        "http://discogs.in/*"],
      "js": ["discogs.js"]
    }
  ]
}
